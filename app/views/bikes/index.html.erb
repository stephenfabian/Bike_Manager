<%= link_to "Bike Index", "/bikes" %>
<%= link_to "Shop Index", "/shops" %>


<h1>BIKES</h1>
<% @bikes.each do |bike| %>
  <p>Name: <%= bike.name %><p>
  <p>Bike id: <%= bike.id %><p>
  <p>Full Suspension: <%= bike.full_suspension %><p>
  <p>Travel Length: <%= bike.travel_length %><p> 
  <p>Created at date: <%= bike.created_at %><p>
  <p>UPdated at date: <%= bike.updated_at %><p>
<%end%>